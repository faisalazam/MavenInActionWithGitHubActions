<?xml version="1.0" encoding="UTF-8"?>

<project name="${project.name}">
    <publishDate format="yyyy-MM-dd" position="right"/>
    <version position="right"/>

    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>2.0.0-M6</version>
    </skin>

    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <!-- The following customisation will add 'Fork me on GitHub' ribbon in the top right corner -->
            <gitHub>
                <projectId>faisalazam/usercrudwithjwt</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>gray</ribbonColor>
            </gitHub>
        </fluidoSkin>
    </custom>

    <body>
        <links>
            <item name="${project.name}" href="https://github.com/faisalazam/usercrudwithjwt"/>
        </links>
        <breadcrumbs>
            <item name="Muhammad Faisal" href="https://github.com/faisalazam"/>
        </breadcrumbs>
        <menu>
            <item name="README" href="README.html"/>
        </menu>
        <menu ref="reports" inherit="top"/>
        <menu>
            <item name="JaCoCo Combined" href="jacoco-merged/index.html"/>
        </menu>
    </body>
</project>